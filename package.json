{
  "name": "rerum-imperium",
  "version": "1.0.0",
  "description": "Rerum Imperium SPA/PWA",
  "repository": {
    "type": "git",
    "url": "https://github.com/AymericBebert/rerum-imperium"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:serve": "ng build && npx http-server -p 4600 -c-1 dist/rerum-imperium/browser",
    "build:dev": "ng build --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "extract": "ngx-translate-extract --input './src/**/!(*.spec).{html,ts}' --output ./src/assets/i18n/{en,fr}.json --clean --sort --format namespaced-json --format-indentation '  ' --string-as-default-value 'MISSING TRANSLATION'"
  },
  "author": "Aymeric BERNARD",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^17.0.5",
    "@angular/cdk": "^17.0.2",
    "@angular/common": "^17.0.5",
    "@angular/compiler": "^17.0.5",
    "@angular/core": "^17.0.5",
    "@angular/forms": "^17.0.8",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.5",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@angular/service-worker": "^17.0.8",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "rxjs": "~7.8.1",
    "socket.io-client": "~4.7.2",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "^17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@angular/language-service": "^17.0.8",
    "@colsen1991/ngx-translate-extract-marker": "^2.0.8",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vendure/ngx-translate-extract": "^9.0.3",
    "eslint": "^8.56.0",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "ts-node": "~10.9.1",
    "typescript": "~5.2.2"
  }
}
