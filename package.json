{
  "name": "rerum-imperium",
  "version": "1.0.0",
  "description": "Rerum Imperium SPA/PWA",
  "repository": {
    "type": "git",
    "url": "https://github.com/AymericBebert/rerum-imperium"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:stats": "ng build --configuration production --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "extract": "ngx-translate-extract --input './src/**/!(*.spec).{html,ts}' --output ./src/assets/i18n/{en,fr}.json --clean --sort --format namespaced-json --format-indentation '  ' --string-as-default-value 'MISSING TRANSLATION'"
  },
  "author": "Aymeric BERNARD",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^15.1.5",
    "@angular/cdk": "^15.1.5",
    "@angular/common": "^15.1.5",
    "@angular/compiler": "^15.1.5",
    "@angular/core": "^15.1.5",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^15.1.5",
    "@angular/material": "^15.1.5",
    "@angular/platform-browser": "^15.1.5",
    "@angular/platform-browser-dynamic": "^15.1.5",
    "@angular/router": "^15.1.5",
    "@angular/service-worker": "^15.1.5",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "rxjs": "~7.8.0",
    "socket.io-client": "~4.6.1",
    "tslib": "^2.5.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.1.6",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.1.6",
    "@angular/compiler-cli": "^15.1.5",
    "@angular/language-service": "^15.1.5",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@types/jasmine": "~4.3.1",
    "@types/node": "^18.14.0",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "jasmine-core": "~4.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.5",
    "webpack-bundle-analyzer": "^4.8.0"
  }
